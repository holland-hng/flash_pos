    "version": "0.2.0",
    "configurations": [
        {
            "name": "flash_manager",
            "cwd": "app/flash_manager",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Staff Staging",
            "cwd": "app/flash_staff",
            "request": "launch",
            "type": "dart",
            "args": [
                "run",
                "--flavor",
                "staging",
                "-t",
                "lib/main_staging.dart",
            ]
        },
        {
            "name": "Staff Prod",
            "cwd": "app/flash_staff",
            "request": "launch",
            "type": "dart",
            "args": [
                "run",
                "--flavor",
                "production",
                "-t",
                "lib/main_production.dart"
            ]
        },
    ]
}